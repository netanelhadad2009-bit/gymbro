{
  "name": "@fitjourney/web",
  "private": true,
  "scripts": {
    "dev": "node ../../scripts/ensure-port-3000.mjs && next dev -p 3000 -H 0.0.0.0",
    "dev:local": "node ../../scripts/ensure-port-3000.mjs && next dev -p 3000",
    "dev:web": "node ../../scripts/ensure-port-3000.mjs && next dev -p 3000",
    "dev:fast": "node ../../scripts/ensure-port-3000.mjs && TS_NODE_TRANSPILE_ONLY=1 NEXT_TELEMETRY_DISABLED=1 next dev -H 0.0.0.0 -p 3000",
    "log:workout": "node ../../scripts/ensure-port-3000.mjs && cross-env LOG_PROMPT=1 USE_MOCK_WORKOUT=0 next dev -H 0.0.0.0 -p 3000",
    "log:nutrition": "node ../../scripts/ensure-port-3000.mjs && cross-env LOG_PROMPT=1 USE_MOCK_NUTRITION=0 next dev -H 0.0.0.0 -p 3000",
    "mock:all": "node ../../scripts/ensure-port-3000.mjs && cross-env USE_MOCK_WORKOUT=1 USE_MOCK_NUTRITION=1 next dev -H 0.0.0.0 -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "import-exercises": "tsx scripts/import_exercises.ts",
    "export-exercises": "tsx scripts/export_exercises.ts",
    "cap": "cap",
    "cap sync ios": "cap sync ios",
    "cap sync android": "cap sync android",
    "cap open ios": "cap open ios",
    "cap open android": "cap open android",
    "ios:clean": "xcrun simctl shutdown all || true && rm -rf ios/App/build ~/Library/Developer/Xcode/DerivedData/*",
    "android:clean": "rm -rf android/app/build android/.gradle android/.idea",
    "e2e:install": "playwright install --with-deps",
    "e2e:stability": "cross-env NEXT_PUBLIC_CHAOS=1 playwright test e2e/stability.spec.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@capacitor-mlkit/barcode-scanning": "^7.3.0",
    "@capacitor/app": "^7.1.0",
    "@capacitor/browser": "^7.0.2",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/core": "^7.4.3",
    "@capacitor/dialog": "^7.0.2",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/network": "^7.0.2",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@capgo/capacitor-social-login": "^7.18.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-visually-hidden": "^1.2.4",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@use-gesture/react": "^10.3.1",
    "@zxing/browser": "^0.1.5",
    "@zxing/library": "^0.21.3",
    "chart.js": "^4.5.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "clsx": "^2.1.1",
    "cordova-plugin-purchase": "^13.12.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.22",
    "googleapis": "^148.0.0",
    "lucide-react": "^0.544.0",
    "mixpanel-browser": "^2.72.0",
    "next": "14.2.35",
    "openai": "^6.3.0",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.7",
    "resend": "^6.5.2",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2",
    "web-push": "^3.6.7",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.3",
    "@playwright/test": "^1.56.1",
    "@types/node": "^20.11.30",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.6.2",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "jose": "^6.1.0",
    "postcss": "^8.5.6",
    "remove-markdown": "^0.6.2",
    "tailwindcss": "^3",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  }
}
