{
  "name": "@gymbro/web",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:fast": "TS_NODE_TRANSPILE_ONLY=1 NEXT_TELEMETRY_DISABLED=1 next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "import-exercises": "tsx scripts/import_exercises.ts",
    "export-exercises": "tsx scripts/export_exercises.ts",
    "ios:sync": "cap sync ios",
    "ios:run": "cap run ios --target=\"B0DA3FF2-67CD-4C7A-BF28-EDC5519C562D\"",
    "ios:open": "cap open ios",
    "ios:sim": "CAP_TARGET=sim pnpm exec cap sync ios && pnpm exec cap run ios",
    "ios:dev": "CAP_TARGET=device pnpm exec cap sync ios && pnpm exec cap open ios"
  },
  "dependencies": {
    "@capacitor/core": "^7.4.3",
    "@capacitor/dialog": "^7.0.2",
    "@capacitor/ios": "^7.4.3",
    "@capacitor/push-notifications": "^7.0.3",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-toast": "^1.2.15",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "@use-gesture/react": "^10.3.1",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.544.0",
    "next": "14.2.12",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.7",
    "web-push": "^3.6.7",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@types/node": "^20.11.30",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.66",
    "@types/web-push": "^3.6.4",
    "autoprefixer": "^10.4.21",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "jose": "^6.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  }
}
