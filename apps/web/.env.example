# Supabase
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Mixpanel Analytics
NEXT_PUBLIC_MIXPANEL_TOKEN=your_mixpanel_project_token_here
# Session Replay sampling rate (0-100), default 15% in production
NEXT_PUBLIC_MIXPANEL_REPLAY_SAMPLE=15

# AppsFlyer (mobile attribution)
NEXT_PUBLIC_APPSFLYER_DEV_KEY=your_appsflyer_dev_key_here
NEXT_PUBLIC_APPSFLYER_APP_ID=your_app_id_here

# OpenAI / Anthropic
OPENAI_API_KEY=your_openai_key_here
ANTHROPIC_API_KEY=your_anthropic_key_here

# Push Notifications (APNs)
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apple_team_id
APNS_KEY_P8=base64_encoded_p8_key

# Resend (Email)
RESEND_API_KEY=your_resend_api_key

# Google Sheets Export (for user tracking)
# Create a Google Service Account and share the spreadsheet with the service account email
GOOGLE_SHEETS_CLIENT_EMAIL=your-service-account@project.iam.gserviceaccount.com
GOOGLE_SHEETS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id_here
